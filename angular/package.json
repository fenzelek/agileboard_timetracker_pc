{
  "name": "core",
  "version": "0.8.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:electron": "ng serve --configuration=electron",
    "build": "ng build",
    "build:electron": "ng build --configuration=electron",
    "build:electron:prod": "ng build --configuration=electron-prod",
    "watch:electron": "ng build --watch --configuration=electron",
    "run:electron": "cd ../electron && npm start",
    "electron": "npm run build:electron && npm run run:electron",
    "build:cordova": "ng build --configuration=cordova && node ./build-cordova",
    "build:cordova:prod": "ng build --configuration=cordova-prod && node ./build-cordova",
    "watch:cordova": "ng build --watch --configuration=cordova",
    "run:cordova": "cd ../cordova && npm run emulate:android",
    "cordova": "npm run build:cordova && npm run run:cordova",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
